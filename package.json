{
    "name": "@lws-analytics/script",
    "version": "1.0.0",
    "description": "TypeScript types and utilities for LWS Analytics",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist",
        "global.d.ts"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "analytics",
        "lws",
        "typescript",
        "tracking"
    ],
    "author": "LWS",
    "license": "MIT",
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lws-analytics/script.git"
    },
    "bugs": {
        "url": "https://github.com/lws-analytics/script/issues"
    },
    "homepage": "https://github.com/lws-analytics/script#readme"
}
